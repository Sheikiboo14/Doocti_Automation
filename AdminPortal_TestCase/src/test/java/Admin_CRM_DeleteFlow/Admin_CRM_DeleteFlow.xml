<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CRM Deletion Flow Testing">
	
	<test name="Preview Campaign Deletion">
		
		<parameter name="campaign_Name" value="Preview Campaign"/>	
