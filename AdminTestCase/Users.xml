<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="User Module Testing">
	
  <test name ="User Creation">
	<classes>      
      <class name="User.Create_User">
		  <methods>
			  <exclude name="Create_User"></exclude>
		  </methods></class>
      
    </classes>
  </test> <!-- Test -->


<test name="User Updation ">
	<classes>
		<class name="User.Updat_User">
			<methods>
				<include name="Update_Status"></include>
				<exclude name="Update_UserMobileNo"></exclude>
				<exclude name="Update_UserEmail"></exclude>
				<exclude name="Update_Role"></exclude>
			</methods>
			
		</class>
		
		
	</classes>
</test>
   
</suite> <!-- Suite -->


